			"ipv6": {
				"addressing": "dynamic"
			},
			"ssids": {
				"mesh": {
					"template": {
						"mode": "batman-adv",
						"key": "meshmesh"
					},
					"ssid": "mesh",
					"wifi-radios": [
						"5G"
					],
					"bss-mode": "mesh"
				},
				"main": {
					"template": {
						"mode": "encrypted",
						"security": "compatibility",
						"key": "aaaaaaaa"
					},
					"ssid": "OpenWrt",
					"wifi-radios": [
						"2G", "5G"
					],
				}
			}
		}
	},

	"services": {
		"ssh": {
			"port": 22
		}
	}
