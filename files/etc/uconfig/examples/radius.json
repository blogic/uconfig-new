{
	"uuid": 2,

	"radios": {
		"2G": {
			"channel-mode": "HE",
			"channel-width": 20
		},
		"5G": {
			"channel-mode": "HE",
			"channel-width": 80,
			"channel": 36
		}
	},

	"definitions": {
		"radius-servers": {
			"radius": {
				"authentication": {
					"host": "192.168.42.78",
					"port": 1812,
					"secret": "secret"
				},
				"accounting": {
					"host": "192.168.42.78",
					"port": 1813,
					"secret": "secret"
				}
			}
		}
	},

	"interfaces": {
		"wan": {
			"role": "upstream",
			"services": [ "mdns", "ssh" ],
			"ports": {
				"wan*": "auto",
				"lan*": "auto"
			},
			"ipv4": {
				"addressing": "dynamic"
			},
			"ipv6": {
				"addressing": "dynamic"
			},
			"ssids": {
				"main": {
					"template": {
						"mode": "enterprise",
						"security": "maximum",
						"radius-server": "radius"
					},
					"ssid": "OpenWrt-wpa2",
					"wifi-radios": [
						"2G", "5G"
					],
				}
			}
		}
	},

	"services": {
		"ssh": {
			"port": 22
		}
